{
  "use": [
    "postcss-import",
    "postcss-css-variables",
    "postcss-nested",
    "postcss-conditionals",
    "postcss-custom-media",
    "css-mqpacker",
    "perfectionist",
    "autoprefixer"
  ],
  "input": "src/css/index.pcss",
  "output": "src/css/bundle.min.css",
  "local-plugins": true,
  "autoprefixer": {
    "browsers": "> 5%"
  },
  "perfectionist": {
    "format": "compressed"
  }
}